# Database Connection
# Replace with your Supabase PostgreSQL connection string
DATABASE_URL="postgresql://username:password@host:port/database?pgbouncer=true"

# Application Configuration
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# Vercel Environment (auto-populated in production)
VERCEL_ENV="development"

# Optional: Direct database connection (without connection pooling)
# Use this for Prisma migrations and schema operations
DIRECT_URL="postgresql://username:password@host:port/database"